{
  "author": "Sergio Cruz",
  "name": "react-connect4",
  "version": "0.2.0",
  "description": "Connect 4 game implemented in React",
  "main": "app/client.js",
  "scripts": {
    "start": "gulp watch",
    "test": "jest",
    "test-debug": "node-debug --nodejs --harmony ./node_modules/jest-cli/bin/jest.js --runInBand"
  },
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^29.2.2",
    "babelify": "^10.0.0",
    "browser-sync": "^2.8.0",
    "browserify": "^17.0.0",
    "classnames": "^2.2.0",
    "eslint": "^1.0.0",
    "eslint-plugin-react": "^3.1.0",
    "exorcist": "^0.4.0",
    "gulp": "^4.0.2",
    "gulp-sass": "^3.2.1",
    "gulp-util": "^3.0.6",
    "jest-cli": "^29.2.2",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.0",
    "react-dom": "^0.14.0",
    "reactify": "^1.1.1",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.3.0"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageOnlyFrom": {
      "app/components/app.js": true,
      "app/components/connect-4/board.js": true
    },
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js"
    ],
    "unmockedModulePathPatterns": [
      "react"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sergiocruz/react-connect4.git"
  },
  "engines": {
    "node": "4.2.x",
    "npm": "2.14.x"
  }
}
